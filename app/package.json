{
  "name": "lastenkirkko-app",
  "scripts": {
    "start": "node node_modules/gulp/bin/gulp.js",
    "test": "npm run build && node node_modules/gulp/bin/gulp.js test",
    "lint": "node node_modules/gulp/bin/gulp.js lint",
    "build": "npm run build:dev",
    "build:dev": "npm install && node node_modules/gulp/bin/gulp.js build:dev",
    "build:prod": "npm install && node node_modules/gulp/bin/gulp.js build:prod",
    "deploy": "npm install && npm run lint && npm test && node node_modules/gulp/bin/gulp.js build:prod && node node_modules/gulp/bin/gulp.js zip && node node_modules/gulp/bin/gulp.js upload",
    "deploy:skip-tests": "npm install && npm run lint && node node_modules/gulp/bin/gulp.js build:prod && node node_modules/gulp/bin/gulp.js zip && node node_modules/gulp/bin/gulp.js upload",
    "postinstall": "bower install"
  },
  "devDependencies": {
    "bower": "^1.3.3",
    "browser-sync": "^2.7.12",
    "connect-modrewrite": "^0.8.1",
    "dotenv": "^1.2.0",
    "gulp": "3.9.0",
    "gulp-aws": "0.0.4",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^0.14.0",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^0.3.13",
    "gulp-ng-annotate": "^0.5.2",
    "gulp-ng-constant": "^0.3.0",
    "gulp-ng-templates": "0.0.6",
    "gulp-protractor": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-run-sequence": "^0.3.2",
    "gulp-sass": "^2.1.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "3.0.7",
    "gulp-zip": "3.0.2",
    "lodash": "3.9.3",
    "main-bower-files": "2.9.0",
    "node-sass": "^3.4.2",
    "protractor": "^2.1.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.0"
  }
}
